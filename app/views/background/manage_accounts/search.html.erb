<%= render 'account_search_high' %>
<br><br>

<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <table class="table table-striped table-hover">
      <tr>
        <th>账号</th>
        <th>姓名</th>
        <th>充值金额</th>
        <th>充值时间</th>
      </tr>

      <% @search.results.each do |account| %>
          <tr>
            <td><%= account.user.email %></td>
            <td><%= account.user.name %></td>
            <td><%= account.mount %></td>
            <td><%= get_time account.created_at %></td>
          </tr>
      <% end %>
    </table>
  </div>
</div>

